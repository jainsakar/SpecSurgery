<% include partials/header.ejs%>   
<div class="container">
    <div class="row">
        <div class="col-md-3">
            <p class="lead">Specube</p>
            <div class="list-group">
                <li class="list-group-item active">Info 1</li>
                <li class="list-group-item">Info 2</li>
                <li class="list-group-item">Info 3</li>
            </div>
        </div>
        <div class="col-md-9">
            <div class="thumbnail">
                <img class="img-responsive" src="<%= hospital.image %>">
                <div class="caption-full">
                    <h4 class="pull-right"><%= hospital.location %></h4>
                    <h4><a><%=hospital.name%></a></h4>
                    <p>ICU-Rent: <%= hospital.icuRent %></p>
                    <p>General-Rent: <%= hospital.generalRent %></p>
                    <p>Year of Establishment: <%= hospital.yearOfEstablishment%></p>
                </div>
            </div>
            <div class="well">
                <hr>
                <% hospital.surgery.forEach(function(surgery) { %>
                    <div class="row">
                        <div class="col-md-12">
                           
                            <strong><%= surgery.disease %></strong>
                            <p>Cost: <%= surgery.cost %></p>  
                        
                            
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
</div>
    
    
    
    
<% include partials/footer.ejs%>    